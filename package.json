{
  "name": "sehatq-web-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build:docs-dev": "turbo run build:sb-dev --scope=docs --no-deps --include-dependencies",
    "build:docs-sanity": "turbo run build:sb-sanity --scope=docs --no-deps --include-dependencies",
    "build:docs": "turbo run build:sb --scope=docs --no-deps --include-dependencies",
    "build:tokoq-dev": "turbo run build:dev --scope=tokoq --no-deps --include-dependencies",
    "build:tokoq-sanity": "turbo run build:sanity --scope=tokoq --no-deps --include-dependencies",
    "build:tokoq": "turbo run build --scope=tokoq --no-deps --include-dependencies",
    "build:sehatq-dev": "turbo run build:dev --scope=sehatq --no-deps --include-dependencies",
    "build:sehatq-sanity": "turbo run build:sanity --scope=sehatq --no-deps --include-dependencies",
    "build:sehatq-rc": "turbo run build:rc --scope=sehatq --no-deps --include-dependencies",
    "build:sehatq": "turbo run build --scope=sehatq --no-deps --include-dependencies",
    "build:fuse-dev": "turbo run build:dev --scope=fuse --no-deps --include-dependencies",
    "build:fuse-sanity": "turbo run build:sanity --scope=fuse --no-deps --include-dependencies",
    "build:fuse-rc": "turbo run build:rc --scope=fuse --no-deps --include-dependencies",
    "build:fuse": "turbo run build --scope=fuse --no-deps --include-dependencies",
    "build:paymentq-dev": "turbo run build:dev --scope=paymentq --no-deps --include-dependencies",
    "build:paymentq-sanity": "turbo run build:sanity --scope=paymentq --no-deps --include-dependencies",
    "build:paymentq-rc": "turbo run build:rc --scope=paymentq --no-deps --include-dependencies",
    "build:paymentq": "turbo run build --scope=paymentq --no-deps --include-dependencies",
    "dev:docs-dev": "turbo run dev:sb-dev --scope=docs --parallel --no-cache --no-deps --include-dependencies",
    "dev:docs-sanity": "turbo run dev:sb-sanity --scope=docs --parallel --no-cache --no-deps --include-dependencies",
    "dev:docs": "turbo run dev:sb --scope=docs --parallel --no-cache --no-deps --include-dependencies",
    "dev:tokoq-dev": "turbo run dev:dev --scope=tokoq --parallel --no-cache --no-deps --include-dependencies",
    "dev:tokoq-sanity": "turbo run dev:sanity --scope=tokoq --parallel --no-cache --no-deps --include-dependencies",
    "dev:tokoq": "turbo run dev --scope=tokoq --parallel --no-cache --no-deps --include-dependencies",
    "dev:sehatq-dev": "turbo run dev:dev --scope=sehatq --parallel --no-cache --no-deps --include-dependencies",
    "dev:sehatq-sanity": "turbo run dev:sanity --scope=sehatq --parallel --no-cache --no-deps --include-dependencies",
    "dev:sehatq-rc": "turbo run dev:rc --scope=sehatq --parallel --no-cache --no-deps --include-dependencies",
    "dev:sehatq": "turbo run dev --scope=sehatq --parallel --no-cache --no-deps --include-dependencies",
    "dev:fuse-dev": "turbo run dev:dev --scope=fuse --parallel --no-cache --no-deps --include-dependencies",
    "dev:fuse-sanity": "turbo run dev:sanity --scope=fuse --parallel --no-cache --no-deps --include-dependencies",
    "dev:fuse-rc": "turbo run dev:rc --scope=fuse --parallel --no-cache --no-deps --include-dependencies",
    "dev:fuse": "turbo run dev --scope=fuse --parallel --no-cache --no-deps --include-dependencies",
    "dev:paymentq-dev": "turbo run dev:dev --scope=paymentq --parallel --no-cache --no-deps --include-dependencies",
    "dev:paymentq-sanity": "turbo run dev:sanity --scope=paymentq --parallel --no-cache --no-deps --include-dependencies",
    "dev:paymentq-rc": "turbo run dev:rc --scope=paymentq --parallel --no-cache --no-deps --include-dependencies",
    "dev:paymentq": "turbo run dev --scope=paymentq --parallel --no-cache --no-deps --include-dependencies",
    "gen:theme-typings": "chakra-cli tokens ./packages/sehatq-components/src/user-interfaces/theme",
    "lint": "turbo run lint --concurrency=1",
    "test": "turbo run test",
    "test:get-lcov": "turbo run test:get-lcov",
    "turbo": "turbo run",
    "turbo:prune": "turbo prune",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "pre-commit": "turbo run lint:staged --concurrency=1",
    "release": "./release.sh",
    "release:fuse": "./release-fuse.sh"
  },
  "devDependencies": {
    "@chakra-ui/cli": "1.7.0",
    "eslint": "7.32.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "turbo": "1.1.0"
  },
  "engines": {
    "node": "^16.12.0"
  },
  "packageManager": "yarn@1.22.17",
  "msw": {
    "workerDirectory": "packages/sehatq-components/public"
  }
}
